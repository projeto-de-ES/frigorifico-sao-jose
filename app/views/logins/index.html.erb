<p id="notice"><%= notice %></p>
<body>
<%= form_with scope: :logins, url:logins_path, local:true do%>
<br/>
  <label>Login:
    <%= text_field_tag :login, nil, id:"login", placeholder:"Login" %>
  </label>
  <br />
  <label>Senha:
    <%= text_field_tag :senha, nil, id:"senha", type:"password", placeholder:"Senha" %>
  </label>
  <br />
  <%= submit_tag 'entrar' %>
<%end%>
<br/>
<%= link_to 'Inicio', root_path %>
</body>